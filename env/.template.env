# Environment
NODE_ENV=dev
DEPLOY_ENV=dev

# Server Configuration
PORT=3000
CORS_ALLOWED_ORIGINS=*
BODY_PARSER_JSON_SIZE_LIMIT=10mb

# API Configuration
API_DEFAULT_VERSION=1
API_LOG_LEVEL=debug

# Base URLs
DEV_BASE_URL=http://localhost:3000
STAGING_BASE_URL=http://localhost:3000
PRODUCTION_BASE_URL=http://localhost:3000

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long-change-in-production
JWT_EXPIRES_IN=1d

